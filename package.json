{
    "name": "dentaflow",
    "version": "1.0.0",
    "type": "module",
    "description": "Application de gestion dentaire sécurisée",
    "main": "src/api/server.js",
    "scripts": {
        "start": "node src/api/server.js",
        "dev": "nodemon src/api/server.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
        "test:watch": "jest --watch",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "security": "npm audit --audit-level=high",
        "docs": "swagger-jsdoc -d docs/swagger-definition.cjs \"src/api/**/*.js\" -o docs/swagger.json"
    },
    "keywords": [
        "dental",
        "healthcare",
        "devsecops"
    ],
    "author": "Votre Équipe",
    "license": "MIT",
    "devDependencies": {
        "@eslint/json": "^0.14.0",
        "eslint": "^9.39.2",
        "eslint-plugin-security": "^3.0.1",
        "jest": "^29.0.0",
        "nodemon": "^3.0.0"
    },
    "dependencies": {
        "express": "^4.18.0",
        "swagger-jsdoc": "^6.0.0",
        "swagger-ui-express": "^5.0.0",
        "winston": "^3.0.0"
    }
}